 <div class="col-md-4">
  <div class="card">
    <img
      class="card-img-top img"

      src={{image_url}}
      alt={{name}}
  
    />
    <div class="card-body">
      <h5 class="card-title">{{name}}</h5>
      <p class="card-text">{{description}}</p>
      <p>{{tagline}}</p>
    </div>
  </div>
</div>